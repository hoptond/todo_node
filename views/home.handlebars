<h1>Blue Todo</h1>
<form method="post">
    <ul>
        {{#each todo}}
            <li>
                <input type="checkbox" id='1' name="1">
                <span>{{this}}</span>
            </li>
        {{/each}}
        <li>
            <input name="new_task" type="text" placeholder="Enter a new task here...">
        </li>
    </ul>
    <input type="submit" value="SUBMIT">
</form>